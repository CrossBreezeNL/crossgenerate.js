{
    "generate": {
        "forModel":"../models/example-source.json",
        "forModelNodePaths":[
            {
                "modelJsonPath":"system.tables.*",
                "templates":[
                        { "outputNameFormat": "tbl_{{table.name}}.sql", "inputTemplate": "CreateView.mst" },
                        { "outputNameFormat": "vw_{{table.name}}.sql", "inputTemplate": "CreateTable.mst" }
                ]
            },
            {
                "modelJsonPath":"system",
                "templates":[
                    "schema.mst"
                ]
            }
        ]
    }
}